programa
{
	
	funcao inicio()
	{
		real conversao, dolares, cotacao
		
		escreva("Quantidade de dólares que você possui: US$")
		leia(dolares)

		escreva("Valor da cotação do dólar: ")
		leia(cotacao)

		conversao = dolares / cotacao

		escreva("Seus dólares equivalem a R$: ", conversao, "\n")
		
		
	}
	
}
